{
  "manifest_version": 3,
  "name": "wise 调试",
  "version": "1.0",
  "author": ["baozj", "chenshuo"],
  "description": "可以进行字体以及暗黑模式的调试，并扩展 San 卡标记功能。",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs",
    "webNavigation",
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["icons/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
