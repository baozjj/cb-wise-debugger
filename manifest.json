{
  "manifest_version": 3,
  "name": "wise 调试",
  "version": "1.0",
  "author": ["baozj", "chenshuo"],
  "description": "可以进行字体以及暗黑模式的调试，并扩展 San 卡标记功能。",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs",
    "nativeMessaging"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }
  ]
}
